Algoritmo restaNumeros
	Definir numberOne, numberTwo, finalResult Como Entero;
	Escribir "Escriba su primer numero (ten en cuenta que se restara el primero con el segundo)";
	Leer numberOne;
	Escribir "Escriba su segundo numero";
	Leer numberTwo;
	finalResult <- numberOne - numberTwo;
	Escribir "Su resultado final es:", finalResult;
FinProceso
